<Window x:Class="StyleBox.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:StyleBox"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button Content="LoadData" HorizontalAlignment="Center" Height="33" Margin="0,278,0,0" VerticalAlignment="Top" Width="100" Click="Button_Click"/>
        <DataGrid HorizontalAlignment="Left" Height="171" Margin="123,64,0,0" VerticalAlignment="Top" Width="535" d:ItemsSource="{d:SampleData ItemCount=5}"/>

    </Grid>
</Window>
